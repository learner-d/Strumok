<Application x:Class="StrumokApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:StrumokApp"
             xmlns:bnd="clr-namespace:StrumokApp.Binding"
             StartupUri="View/MainWindow.xaml"
             DispatcherUnhandledException="StrumokApp_DispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <BooleanToVisibilityConverter x:Key="boolToVisibilityConverter"/>
            <bnd:ArrULongToHexConverter x:Key="arrULArrToStrConv"/>
            <bnd:KeyPartConverter x:Key="keyPartConverter"/>
            <bnd:StrumokKeyConverter x:Key="strumokKeyConverter"/>
            <bnd:InverseBooleanConverter x:Key="invBoolConv"/>
            <bnd:NullToVisibillityConverter x:Key="nullToVisibilityConv"/>
            <bnd:MessageLevelToBrushConverter x:Key="msgLvlToBrushConv"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
